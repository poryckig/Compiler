class Point {
    int x;
    int y;
    
    Point(int x_val, int y_val) {
        this.x = x_val;
        this.y = y_val;
    }
    
    int sum() {
        return this.x + this.y;
    }

    int getX() {
        return this.x;
    }
    
    int getY() {
        return this.y;
    }
    
    void setX(int new_x) {
        this.x = new_x;
    }
    
    void setY(int new_y) {
        this.y = new_y;
    }
};

void testClassMethods() {
    Point p = Point(10, 20);
    
    print "X value:";
    print p.getX();
    
    print "Y value:";
    print p.getY();
    
    print "Sum of x and y:";
    print p.sum();
    
    print "Setting new X value to 50";
    p.setX(50);
    
    print "Setting new Y value to 100";
    p.setY(100);
    
    print "New X value:";
    print p.getX();
    
    print "New Y value:";
    print p.getY();
    
    print "New sum of x and y:";
    print p.sum();
}

print "Testing class methods";
testClassMethods();