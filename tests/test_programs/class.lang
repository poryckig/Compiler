class Point {
    int x;
    int y;
    
    Point(int x_val, int y_val) {
        this.x = x_val;
        this.y = y_val;
    }
    
    int getX() {
        return this.x;
    }
    
    int getY() {
        return this.y;
    }
    
    int sum() {
        return this.x + this.y;
    }
};

void testClassMethods() {
    Point p = Point(10, 20);
    
    print "X value:";
    print p.getX();
    
    print "Y value:";
    print p.getY();
    
    print "Sum of x and y:";
    print p.sum();
}

print "Testing class methods";
testClassMethods();